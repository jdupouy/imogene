<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:imo='urn:import:org.imogene.web.client.ui.field.group'>

	<ui:style src="../../../../web/client/css/ImogCss.css">
		.title {
			font-size: 14px;
			font-weight: bold;
		}
	</ui:style>

	<imo:FieldGroupPanel ui:field="syncFieldGroup">

		<g:HorizontalPanel verticalAlignment="middle"
			spacing="5">

			<g:cell horizontalAlignment="right" verticalAlignment="middle"
				width="300px">
				<g:Label ui:field="dateTitle" width="300px" styleName="{style.title}" />
			</g:cell>

			<g:cell horizontalAlignment="left" verticalAlignment="middle"
				width="300px">
				<g:Label ui:field="dateValue" width="300px" styleName="{style.title}" />
			</g:cell>

			<g:cell horizontalAlignment="right" verticalAlignment="middle"
				width="100px">
				<g:Label ui:field="statusTitle" width="100px" styleName="{style.title}" />
			</g:cell>

			<g:cell horizontalAlignment="left" verticalAlignment="middle"
				width="32px">
				<g:Image ui:field="statusValue" width="32px" />
			</g:cell>

			<g:cell horizontalAlignment="right" verticalAlignment="middle"
				width="80px">
				<g:PushButton ui:field="resetButton"
					styleName="{style.imogButton} {style.imogButton2}" />
			</g:cell>

		</g:HorizontalPanel>

	</imo:FieldGroupPanel>


</ui:UiBinder>