<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HorizontalPanel verticalAlignment="middle">

		<g:cell horizontalAlignment="left" verticalAlignment="middle"
			width="32px">
			<g:Label ui:field="statusLabel" width="32px" />
		</g:cell>

		<g:cell horizontalAlignment="left" verticalAlignment="middle"
			width="180px">
			<g:Image ui:field="statusImage" width="32px" />
		</g:cell>

		<g:cell horizontalAlignment="right" verticalAlignment="middle"
			width="80px">
			<g:PushButton ui:field="resetButton" />
		</g:cell>

	</g:HorizontalPanel>


</ui:UiBinder>